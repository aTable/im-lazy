{
    "name": "im-lazy",
    "version": "0.3.2",
    "private": true,
    "scripts": {
        "s": "npm run start",
        "start": "npm run gql-codegen && cross-env HTTPS=true react-scripts start",
        "build": "react-scripts build && npm run generate-docs",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "generate-docs": "typedoc",
        "gql-codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config codegen.yml"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.1.2",
        "@fortawesome/fontawesome-free": "^5.14.0",
        "@types/jest": "26.0.8",
        "@types/jquery": "^3.5.1",
        "@types/jwt-decode": "^2.2.1",
        "@types/react": "16.9.44",
        "@types/react-dom": "16.9.8",
        "@types/react-notification-system": "^0.2.39",
        "@types/react-router-dom": "^5.1.5",
        "axios": "^0.19.2",
        "bootstrap": "^4.5.1",
        "date-fns": "^2.15.0",
        "formik": "^2.1.5",
        "jquery": "^3.5.1",
        "jwt-decode": "^2.2.0",
        "oidc-client": "^1.10.1",
        "popper.js": "^1.16.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-notification-system": "^0.4.0",
        "react-router-dom": "^5.2.0",
        "typescript": "3.9.7",
        "yup": "^0.29.3"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.17.7",
        "@graphql-codegen/introspection": "1.17.7",
        "@graphql-codegen/typescript": "1.17.7",
        "@graphql-codegen/typescript-operations": "1.17.7",
        "@graphql-codegen/typescript-react-apollo": "2.0.1",
        "@types/react-test-renderer": "^16.9.3",
        "@types/yup": "^0.29.4",
        "cross-env": "^7.0.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "graphql": "^15.3.0",
        "node-sass-chokidar": "^1.5.0",
        "prettier": "^2.0.5",
        "react-scripts": "^3.4.3",
        "react-test-renderer": "^16.13.1",
        "typedoc": "^0.18.0"
    }
