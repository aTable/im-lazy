{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Trace", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithThreadId", "WithEnvironment", "WithMachineName" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs\\your-namespace-consoleapp.log"
        }
      },
      {
        "Name": "Trace"
      },
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({MachineName}/{ThreadId}) {SourceContext:l} {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "MyApplicationName": "Your.Namespace.something-or-other"
    }
  },
  "App": {
    "SayHelloWorldThisManyTimes": 10
  }
}