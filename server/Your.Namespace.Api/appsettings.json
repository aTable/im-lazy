{
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/.log",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console",
        "Args": {}
      }
    ]
  },
  "ConnectionStrings": {
    "ConnectionString": "Data Source=./yournamespace.db;"
  },
  "App": {
    "CorsPolicyName": "_corsPolicyName",
    "WebClientOrigin": "https://localhost:3000",
    "AuthorizationServerUri": "http://localhost:9090"
  },
  "AllowedHosts": "*"
}
