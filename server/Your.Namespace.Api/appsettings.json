{
  "Serilog": {
    "Using": ["Serilog.Sinks.SQLite"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "log.db",
          "tableName": "logs",
          "storeTimestampInUtc": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "ConnectionString": "Data Source=./yournamespace.db;"
  },
  "App": {
    "ApiName": "MyAwesome-API",
    "CorsPolicyName": "_corsPolicyName",
    "WebClientOrigin": "https://localhost:3000",
    "AuthorizationServerUri": "http://localhost:9090"
  },
  "AllowedHosts": "*"
}
