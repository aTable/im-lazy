{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Trace" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%SYSTEMDRIVE%\\logs\\Your.Namespace.WebApi.log"
        }
      },
      {
        "Name": "Trace"
      }
    ]
  },
  "AllowedHosts": "*",
  "App": {
    "Name": "Your.Namespace.WebApi",
    "Nested": {
      "NestedValue": 531
    }
  }
}