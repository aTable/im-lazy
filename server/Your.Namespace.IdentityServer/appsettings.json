{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:9082"
      },
      "Https": {
        "Url": "https://0.0.0.0:9083"
      }
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.SQLite"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "log.db",
          "tableName": "logs",
          "storeTimestampInUtc": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "IdentityConnectionString": "Data source=./yournamespace-sts.db"
  },
  "App": {
    "WebClientOrigin": "",
    "IsRunSeed": true,
    "AzureKeyVault": {
      "CertName": "",
      "Uri": ""
    },
    "AzureOIDC": {
      "Authority": "",
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "AllowedHosts": "*"
}
