{
  "Serilog": {
    "Using": ["Serilog.Sinks.SQLite"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "log.db",
          "tableName": "logs",
          "storeTimestampInUtc": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "IdentityConnectionString": ""
  },
  "App": {
    "WebClientOrigin": "",
    "IsRunSeed": false,
    "AzureKeyVault": {
      "CertName": "",
      "Uri": ""
    },
    "AzureOIDC": {
      "Authority": "",
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "AllowedHosts": "*"
}
